%table.admin.full
  %tr
    %th Name
    %th Email
    %th Github
    %th Twitter
    %th Project
  - students.each do |student|
    %tr
      %td= link_to student.name, admin_user_path(student)
      %td= student.email
      %td= github_account_link student.github_account_name
      %td= twitter_account_link student.twitter_account_name
      %td= github_project_link student.project_url